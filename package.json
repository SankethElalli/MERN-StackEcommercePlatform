{
  "name": "westernstreet",
  "version": "2.0.0",
  "type": "module",
  "description": "Multi-vendor marketplace built with the MERN stack",
  "main": "backend/server.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm install && npm run build",
    "install-client": "cd frontend && npm install",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "npm run install-client && npm run build",
    "vercel-build": "npm run install-client && npm run build"
  },
  "author": "souravbudke",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.0",
    "markdown-pdf": "^11.0.0",
    "mongoose": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "react-router-bootstrap": "^0.26.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "concurrently": "^7.6.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^2.0.21"
  }
}
